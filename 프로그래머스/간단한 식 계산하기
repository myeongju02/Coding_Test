class Solution {
    public int solution(String binomial) {
        int answer = 0;
        String[] sp = binomial.split(" ");
        int a = Integer.parseInt(sp[0]);
        int b = Integer.parseInt(sp[2]);
        
        switch(sp[1]) {
            case "+": answer = a + b; break;
            case "-": answer = a - b; break;
            case "*": answer = a * b; break;
        }
        return answer;
    }
}
