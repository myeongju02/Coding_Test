import java.util.*;

class Solution {
    public List<String> solution(String[] strArr) {
        List<String> answer = new ArrayList<String>();
        List<String> Arr = new ArrayList<String>(Arrays.asList(strArr));
        
        for(int i=0;i<Arr.size();i++) {
            if(!strArr[i].contains("ad")) answer.add(strArr[i]);
        }
        return answer;
    }
}
